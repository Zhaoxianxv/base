<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:tools="http://schemas.android.com/tools">
	
	<View
		android:layout_width="match_parent"
		android:layout_height="match_parent" />
	
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="350dp"
		android:layout_marginRight="35dp"
		android:layout_marginLeft="35dp"
		android:layout_gravity="center"
		android:background="@drawable/radius5_white"
		>
		<RelativeLayout
			android:id="@+id/rtl_layout_protocol"
			android:layout_width="match_parent"
			android:layout_height="@dimen/margin_90dp"
			android:layout_alignParentBottom="true"
			>
			
			
			<androidx.appcompat.widget.AppCompatTextView
				android:id="@+id/tv_pass_app_protocol"
				android:layout_width="match_parent"
				android:layout_height="45dp"
				tools:text="同意"
				android:textColor="@color/White"
				android:textSize="@dimen/textsize10"
				android:textStyle="bold"
				android:gravity="center"
				android:background="@drawable/radius_bottom_left5_rigth5"
				android:backgroundTint="#B64D52"
				android:layout_alignParentBottom="true"
				/>
			
			<androidx.appcompat.widget.AppCompatTextView
				android:id="@+id/tv_exit_app_protocol"
				android:layout_width="match_parent"
				android:layout_height="45dp"
				tools:text="退出应用"
				android:textSize="@dimen/textsize9"
				android:textStyle="bold"
				android:gravity="center"
				android:textColor="@color/DarkGray"
				android:layout_above="@id/tv_pass_app_protocol"
				android:background="@color/WhiteSmoke"
				/>
		</RelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="20dp"
			android:layout_above="@id/rtl_layout_protocol"
			android:background="@drawable/gradient_rectangle_white_smoke_white_270"
			/>
		<androidx.appcompat.widget.AppCompatTextView
			
			android:id="@+id/tv_title_protocol"
			android:layout_width="wrap_content"
			android:layout_height="40dp"
			tools:text="提示"
			android:text=""
			android:gravity="center"
			android:textSize="16sp"
			android:textColor="@color/app_base_color"
			android:layout_marginTop="@dimen/margin_10dp"
			android:layout_marginBottom="@dimen/margin_10dp"
			android:layout_centerHorizontal="true"
			/>
		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/tv_content_protocol"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			tools:text="提示"
			android:text=""
			android:layout_below="@id/tv_title_protocol"
			android:layout_above="@id/rtl_layout_protocol"
			android:layout_marginBottom="@dimen/margin_5dp"
			android:paddingStart="@dimen/margin_10dp"
			android:paddingEnd="@dimen/margin_10dp"
			android:textSize="@dimen/textsize6"
			android:textColor="#5b5b5b"
			/>
	
	</RelativeLayout>


</FrameLayout>
